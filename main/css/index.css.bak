@-webkit-keyframes eye {
  0% {
    -webkit-transform: rotate(0, 0);
            transform: rotate(0, 0);
  }
  50% {
    -webkit-transform: translate(10px, 0px);
            transform: translate(10px, 0px);
  }
  100% {
    -webkit-transform: translate(0px, 0px);
            transform: translate(0px, 0px);
  }
}
@keyframes eye {
  0% {
    -webkit-transform: rotate(0, 0);
            transform: rotate(0, 0);
  }
  50% {
    -webkit-transform: translate(10px, 0px);
            transform: translate(10px, 0px);
  }
  100% {
    -webkit-transform: translate(0px, 0px);
            transform: translate(0px, 0px);
  }
}


